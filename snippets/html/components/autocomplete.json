{
	"autocomplete": {
		"prefix": ["forge-autocomplete", "<forge-autocomplete>"],
		"body": [
      "<forge-autocomplete>",
				"\t<forge-text-field>",
					"\t\t<label for=\"$2\">${1:Label}</label>",
					"\t\t<input type=\"text\" id=\"${2:id}\">",
				"\t</forge-text-field>",
      "</forge-autocomplete>"
		],
		"description": "Forge autocomplete component with Forge text-field."
  },
	"autocomplete:empty": {
		"prefix": ["forge-autocomplete:empty"],
		"body": [
      "<forge-autocomplete>",
      	"\t$0",
      "</forge-autocomplete>"
		],
		"description": "Forge autocomplete component."
  }
}